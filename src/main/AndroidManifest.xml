<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qrorder">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/shiba93"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shiba93"
        android:supportsRtl="true"
        android:theme="@style/Theme.QROrder">
        <activity android:name=".Restaurant2Activity" />
        <activity android:name=".Restaurant1Activity" />
        <activity android:name=".FirebaseUIActivity" />
        <activity android:name=".ScanActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>